<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<Scene xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane prefHeight="200" prefWidth="200">
      <children>
         <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="200.0" spacing="30.0">
            <children>
               <Text strokeType="OUTSIDE" strokeWidth="0.0" text=" Create new week OR select file to open" />
               <Button fx:id="openFileButton" mnemonicParsing="false" text="Open File" />
            </children>
            <padding>
               <Insets left="10.0" right="10.0" top="20.0" />
            </padding>
         </VBox>
      </children></AnchorPane>
</Scene>
