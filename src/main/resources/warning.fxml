<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<Scene fill="#d76f6f" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane prefHeight="90.0" prefWidth="236.0">
        <children>
            <VBox alignment="CENTER" prefHeight="90.0" prefWidth="250.0">
            <children>
               <Label alignment="CENTER" text="WARNING" />
               <Label text="Exceeding Max!" />
               <Label text="Please edit max or refrain from adding more" />
               <Button fx:id="closeWarning" mnemonicParsing="false" text="close" />
            </children>
            </VBox>
        </children>
    </AnchorPane>
</Scene>
