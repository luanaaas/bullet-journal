<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.TextFlow?>

<AnchorPane prefHeight="50.0" prefWidth="160.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="70.0" prefWidth="160.0">
         <children>
            <Label fx:id="eventName" contentDisplay="CENTER" prefHeight="17.0" prefWidth="168.0" text="Event Name" textAlignment="CENTER">
               <VBox.margin>
                  <Insets left="10.0" top="10.0" />
               </VBox.margin>
               <font>
                  <Font name="Avenir Next Bold" size="13.0" />
               </font>
            </Label>
            <TextFlow fx:id="eventDesc" prefHeight="200.0" prefWidth="200.0">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin>
            </TextFlow>
            <HBox prefHeight="100.0" prefWidth="200.0" spacing="10.0">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" />
               </VBox.margin>
               <children>
                  <Label fx:id="time" text="time">
                     <font>
                        <Font name="Avenir Book" size="10.0" />
                     </font>
                  </Label>
                  <Label fx:id="duration" text="duration">
                     <font>
                        <Font name="Avenir Book" size="10.0" />
                     </font>
                  </Label>
                  <Label fx:id="category" text="No category">
                     <font>
                        <Font name="Avenir Book" size="10.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
